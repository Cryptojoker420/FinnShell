    "@radix-ui/react-tooltip": "^1.1.2",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "2.1.0",
    "connectkit": "1.9.0",
    "nanoid": "^5.0.7",
    "next": "14.2.4",
    "next-themes": "^0.3.0",
