{
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_SITE_URL=http://localhost:3000 next dev --turbo",
    "build": "NEXT_PUBLIC_SITE_URL=http://localhost:3000 next build",
    "start": "NEXT_PUBLIC_SITE_URL=http://localhost:3000 next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "NEXT_PUBLIC_SITE_URL=http://localhost:3000 next build && next start",
    "test:e2e": "playwright test",
    "seed": "node -r dotenv/config ./scripts/seed.mjs",
    "type-check": "tsc --noEmit",
    "format:write": "prettier --write \"{app,lib,components}/**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"{app,lib,components}**/*.{ts,tsx,mdx}\" --cache",
    "fonts:download": "mkdir -p public/fonts && curl -fsSL https://raw.githubusercontent.com/vercel/geist-font/main/fonts/Geist/webfonts/Geist%5Bwght%5D.woff2 -o public/fonts/GeistVariable.woff2 && curl -fsSL https://raw.githubusercontent.com/vercel/geist-font/main/fonts/GeistMono/webfonts/GeistMono%5Bwght%5D.woff2 -o public/fonts/GeistMonoVariable.woff2",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "test:full": "pnpm run supabase:stop && supabase start & wait-on http://localhost:3000 && pnpm exec playwright test",
    "test:e2e:watch": "pnpm exec playwright test --ui"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.6.2",
